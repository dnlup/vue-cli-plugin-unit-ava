  "homepage": "https://github.com/dnlup/vue-cli-plugin-unit-ava#readme",
  "bugs": {
    "url": "https://github.com/dnlup/vue-cli-plugin-unit-ava/issues"
  },
  "engines": {
    "node": ">=8.11"
