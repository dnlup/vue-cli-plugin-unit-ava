  },
  "engines": {
    "node": ">=8.11"
  },
  "author": "dnlup <dwon.dnl@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "javascript-stringify": "^2.0.0"
  },
  "devDependencies": {
    "@vue/cli": "^3.8.2",
